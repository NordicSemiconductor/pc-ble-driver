+nrf52840_xxaa: CFLAGS += -D__STACK_SIZE=2048
+nrf52840_xxaa: ASMFLAGS += -D__HEAP_SIZE=512
+nrf52840_xxaa: ASMFLAGS += -D__STACK_SIZE=2048
+
