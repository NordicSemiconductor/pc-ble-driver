+ASMFLAGS += -DS132
+ASMFLAGS += -DSER_CONNECTIVITY
+ASMFLAGS += -DSER_PHY_HCI
+ASMFLAGS += -DSER_PHY_HCI_USB_CDC
+ASMFLAGS += -DSOFTDEVICE_PRESENT
